<section class="main-section">
  <div class="main-section-title-wrapper">
    <h2>{{session.user.userName}} ({{session.user.email}}) History</h2>
  </div>
  <div class="main-shop-row">
    <div class="col-md-12 w-100">
      <div class="table-responsive w-100">
        <table class="table table-striped table-bordered table-hover w-100">
          <thead class="thead-dark">
            <tr class="text-center">
              <th>Login Date/Time</th>
              <th>Client Information</th>
            </tr>
          </thead>
          <tbody>
            {{#each session.user.loginHistory}}
              <tr>
                <td class="text-center col-md-3">{{this.dateTime}}</td>
                <td>{{this.userAgent}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>

</section>